{{= it.jsonEncoder }} : {{= it.type }} -> JE.Value
{{= it.jsonEncoder }} data =
    []
        {{~ it.members :m }}
        {{? m.required }}
        |> (::) ("{{= m.rawKey }}", data.{{= m.key }} |> ({{= m.value.jsonEncoder }}))
        {{?? true }}
        |> AWS.Encode.optionalMember
            ({{= m.value.jsonEncoder }})
            ("{{= m.rawKey }}", data.{{= m.key }})
        {{?}}
        {{~}}
        |> JE.object
